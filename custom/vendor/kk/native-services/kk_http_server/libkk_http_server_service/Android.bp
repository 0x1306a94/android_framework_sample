
filegroup {
    name: "kk_http_server_service_aidl",
    srcs: [
        "aidl/com/kk/http/IHTTPServerService.aidl",
    ],
    path: "aidl",
}

cc_library_shared {
    name: "libkk_http_server_service_aidl",
    vendor_available: true,
    aidl: {
        export_aidl_headers: true,
        local_include_dirs: [
            "aidl"
        ],
        include_dirs: [
        ],
    },
    srcs: [
        ":kk_http_server_service_aidl",
    ],
    shared_libs: [
        "liblog",
        "libcutils",
        "libutils",
        "libbinder",
    ],
    cflags: [
        "-Werror",
        "-Wall",
        "-Wextra",
    ],
}

cc_library_shared {
    name: "libkk_http_server_service",
    vendor_available: true,
    cflags: [
        "-std=c++11",
        "-fexceptions",
        "-Wno-unused-parameter",
        "-Wno-unused-function",
        "-Wno-unused-label",
    ],
    srcs: [
        "src/**/*.cpp",
    ],
    local_include_dirs: [
        "src"
    ],
    export_shared_lib_headers: [
        "libbinder",
        "libkk_http_server_service_aidl",
    ],
    export_include_dirs: [
        "."
    ],
    shared_libs: [
        "libkk_http_server_service_aidl",
        "libhv",
        "libbase",
        "liblog",
        "libcutils",
        "libutils",
        "libbinder",
    ],
}
